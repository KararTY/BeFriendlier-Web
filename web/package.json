{
  "name": "twitchr",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "start": "node ace serve --watch --node-args=\"--inspect\"",
    "css-build": "node-sass resources/sass/style.scss public/css/stylesheet.css --output-style compressed",
    "lint": "eslint . --ext=.ts",
    "migrate:rollback": "npm run-script build && node ace migration:rollback",
    "migrate:run": "npm run-script build && node ace migration:run",
    "seed": "npm run-script build && node ace db:seed",
    "r:m:s": "npm run-script build && node ace migration:rollback && node ace migration:run && node ace db:seed"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "adonis-preset-ts": "^1.0.4",
    "eslint": "^7.3.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-adonis": "^1.0.12",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "pino-pretty": "^4.0.0",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.5",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^6.9.3",
    "@adonisjs/auth": "^4.2.2",
    "@adonisjs/core": "^5.0.0-preview-rc-1.9",
    "@adonisjs/fold": "^6.3.5",
    "@adonisjs/lucid": "^8.2.2",
    "@adonisjs/session": "^3.0.4",
    "@adonisjs/shield": "^3.0.4",
    "@adonisjs/view": "^2.0.7",
    "bulma": "^0.9.0",
    "feather-icons": "^4.28.0",
    "got": "^11.3.0",
    "node-sass": "^4.14.1",
    "pg": "^8.2.1",
    "phc-argon2": "^1.0.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
